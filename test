WITH u AS (
  SELECT DISTINCT sku_name FROM system.billing.usage
),
p AS (
  SELECT DISTINCT sku_name FROM system.billing.list_prices WHERE currency_code = 'USD'
)
SELECT u.sku_name,
       CASE WHEN p.sku_name IS NULL THEN 'MISSING_IN_PRICES' ELSE 'OK' END AS price_cover
FROM u
LEFT JOIN p USING (sku_name)
ORDER BY 1;
