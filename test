-- What catalog/schema am I on?
SELECT current_catalog() AS catalog, current_schema() AS schema;

-- Does the table exist?
SHOW TABLES IN system.billing;

-- Any rows at all (no date filter)?
SELECT COUNT(*) AS n_rows_total
FROM system.billing.usage;

-- If >0, whatâ€™s the date range present?
SELECT MIN(CAST(usage_date AS DATE)) AS min_d, MAX(CAST(usage_date AS DATE)) AS max_d
FROM system.billing.usage;

-- Peek a few rows to confirm column names
SELECT *
FROM system.billing.usage
LIMIT 20;
